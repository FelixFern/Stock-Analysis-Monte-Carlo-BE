   "source": [
    "def get_return(data):\n",
    "    data['Daily Return'] = (data.iloc[:, 0] - data.iloc[:, 0].shift(1)) / data.iloc[:, 0].shift(1)\n",
    "    # data['Daily Return'] = np.log(data.iloc[:, 0] / data.iloc[:, 0].shift(1))\n",
    "    data = data.dropna()\n",
    "\n",
    "    sns.displot(data['Daily Return'].dropna(), \n",
    "                bins = 50,\n",
    "                color = 'blue', \n",
    "                kde = True)\n",
    "    plt.title('Daily return distribution')\n",
    "    plt.show()\n",
    "\n",
    "    return data"
   ]
